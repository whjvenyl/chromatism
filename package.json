{
  "name": "chromatism",
  "version": "2.6.0",
  "description": "A simple set of utility functions for colours.",
  "main": "dist/chromatism.js",
  "module": "dist/chromatism.esm.js",
  "typings": "./index.d.ts",
  "repository": "toish/chromatism",
  "scripts": {
    "test": "mocha",
    "build": "npm run build:umd && npm run build:es",
    "build:umd": "rollup -c --format umd --output dist/chromatism.js",
    "build:es": "rollup -c --format es --output dist/chromatism.esm.js"
  },
  "keywords": [
    "color",
    "colour",
    "utility",
    "hue",
    "chroma",
    "CMYK",
    "RGB",
    "HSL"
  ],
  "author": "Toish <toish@toi.sh>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "mocha": "^3.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.0.13"
  },
  "dependencies": {}
}
